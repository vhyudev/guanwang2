<script type="text/javascript">
    function login() {
       var username=$("#username").val()
       var password=$("#password").val()

      $.post("/login",{"username":username,"password":password},function (data) {
        if(data=="faile"){
            alert("用户名或密码错误!")
        }else {
            location.href="index.html"
        }
      })
    }
</script>

</body>
</html>